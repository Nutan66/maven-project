pipeline
{
    agent any
    stages
     {
         stage ('scm checkout')
            {
                step(git 'https://github.com/Nutan66/maven-project')
            }
    stage('Please compile the code')

{step
{
    withMaven(jdk: 'localjdk', maven: 'localmaven') {
    sh 'mvn compile'
}

}
    }

}
